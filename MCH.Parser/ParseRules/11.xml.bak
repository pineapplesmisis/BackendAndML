<?xml version="1.0" encoding="utf-8" ?>
<rules>
  <ListProductsUrl>
  <item><![CDATA[https://startkid.ru/]]></item>
  <item><![CDATA[\/([\w_]+)\/([\w_]+)]]></item>
  <item><![CDATA[\/([\w_]+)\/([\w_]+)\?page=(\d+)]]></item>
  <item><![CDATA[\/([\w_]+)\/([\w_]+)\/([\w-]+)]]></item>
  </ListProductsUrl>
  <ProductUrl>
   <item><![CDATA[\/(products)\/([\w_-]+)]]></item>
  </ProductUrl>
  <UrlBase>
  <![CDATA[https://startkid.ru]]>
  </UrlBase>
  <ProductName>
  <![CDATA[//h1[@itemprop='name']]]>
  </ProductName>
 <ProductPrice>
    <![CDATA[//span[contains(@class, 'js-product-price')]]]>
  </ProductPrice>
  <ProductDescription>
    <![CDATA[//div[contains(@class, 'product-meta-information')]/p]]>
  </ProductDescription>
  <ProductImage takenAttrubute="srcset">
    <![CDATA[//picture[@class='relative']/source]]>
  </ProductImage>
</rules>
